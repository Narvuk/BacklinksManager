{% extends 'base.html.twig' %}
{% block AppMenu %}
    {{ include('menus/settingsmenu.html.twig') }}
{% endblock %}
{% block bodytitle %}
    Credits
{% endblock %}
{% block body %}
<div class="row">
            <div class="col-12 col-sm-6 col-md-3">
              <div class="card bg-light">
                <div class="card-header text-muted border-bottom-0">
                </div>
                <div class="card-body pt-0 pb-0">
                  <div class="row">
                   <div class="col-2 text-center">
                      <img src="{{ asset('dist/img/symfony.png') }}" alt="" class="img-circle img-fluid">
                    </div>
                    <div class="col-10">
                      <h2 class="lead"><b>Symfony</b></h2>
                      <p class="text-muted text-sm"><b>Version: </b> 5.0 | <b>License: </b> MIT </p>
                    </div>
                  </div>
                  <p class="text-muted text-sm">
                      Symfony is an Open Source PHP framework for web applications and a set of reusable PHP components.
                      </p>
                </div>
                <div class="card-footer">
                  <div class="text-right">
                    <a href="https://github.com/symfony/symfony" target="_blank" class="btn btn-sm bg-teal">
                      <i class="fab fa-github"></i> Github
                    </a>
                    <a href="https://symfony.com" target="_blank" class="btn btn-sm btn-primary">
                      <i class="fas fa-link"></i> Website
                    </a>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-12 col-sm-6 col-md-3">
              <div class="card bg-light">
                <div class="card-header text-muted border-bottom-0">
                </div>
                <div class="card-body pt-0 pb-0">
                  <div class="row">
                   <div class="col-2 text-center">
                      <img src="{{ asset('dist/img/AdminLTELogo.png') }}" alt="" class="img-circle img-fluid">
                    </div>
                    <div class="col-10">
                      <h2 class="lead"><b>Admin LTE</b></h2>
                      <p class="text-muted text-sm"><b>Version: </b> 3.0 | <b>License: </b> MIT  </p>
                    </div>
                  </div>
                  <p class="text-muted text-sm">
                      open source admin dashboard & control panel theme. Built on top of Bootstrap.
                      </p>
                </div>
                <div class="card-footer">
                  <div class="text-right">
                    <a href="https://github.com/ColorlibHQ/AdminLTE" target="_blank" class="btn btn-sm bg-teal">
                      <i class="fab fa-github"></i> Github
                    </a>
                    <a href="https://adminlte.io/" target="_blank" class="btn btn-sm btn-primary">
                      <i class="fas fa-link"></i> Website
                    </a>
                  </div>
                </div>
              </div>
            </div>
          </div>
{% endblock %}
{% block pageJS %}
    <!-- Additional Javascript or Jquery  -->
{% endblock %}